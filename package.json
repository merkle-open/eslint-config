{
  "name": "@namics/eslint-config",
  "version": "6.1.1",
  "description": "Default configurations for eslint",
  "author": "Namics AG",
  "contributors": [
    "Ernst Ammann <ernst.ammann@namics.com>",
    "Simon Mollweide <simon.mollweide@namics.com>"
  ],
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/namics/eslint-config-namics.git"
  },
  "bugs": {
    "url": "https://github.com/namics/eslint-config-namics/issues"
  },
  "main": "configurations/es6-browser.js",
  "scripts": {
    "lint": "npm run lint:js",
    "lint:js": "eslint .",
    "test": "npm run lint",
    "create-docs": "node bin/write-documentation",
    "prettier": "npm-run-all prettier:*",
    "prettier:es5": "prettier --write \"test/es5-disable-styles/**/*.js\"",
    "prettier:es5node": "prettier --write \"test/es5-node-disable-styles/**/*.js\"",
    "prettier:es6": "prettier --write \"test/es6-disable-styles/**/*.js\"",
    "prettier:es6react": "prettier --write \"test/es6-react-disable-styles/**/*.js\"",
    "prettier:es8": "prettier --write \"test/es8-disable-styles/**/*.js\""
  },
  "keywords": [
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "eslint-config",
    "eslint",
    "eslintconfig",
    "lint",
    "es2015",
    "react",
    "jsx"
  ],
  "devDependencies": {
    "@namics/prettier-config": "0.2.0",
    "eslint": "5.14.1",
    "eslint-plugin-import": "2.16.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4"
  },
  "peerDependencies": {
    "eslint": ">=5.0.0",
    "eslint-plugin-import": ">=2.0.0"
  },
  "dependencies": {
    "babel-eslint": "10.0.1",
    "eslint-find-rules": "3.3.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4"
  }
}
